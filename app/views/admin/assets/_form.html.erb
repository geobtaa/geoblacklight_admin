<% if @asset.errors.any? %>
  <div class="alert alert-danger mb-4" role="alert">
    <h2 class="h4" class="alert-heading"><%= pluralize(@asset.errors.count, "error") %> prohibited this asset from being saved</h2>
    <ol class="mb-0">
      <% @asset.errors.full_messages.each do |msg| %>
